//function to determine if 
function solution(number){
  let sum = 0;
  //Loop up to the number input
  for (let i = number - 1; i > 0; i--) {
    //check if current number is multiple of 3 or 5
    if (i % 3 === 0 || i % 5 === 0) {
      //add all numbers
      sum += i;
    }
  }
  // returns result
  return sum;
}

console.log(solution(33));